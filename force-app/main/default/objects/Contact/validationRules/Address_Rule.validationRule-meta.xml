<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Address_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>ISBLANK(MailingStreet) || ISBLANK( MailingCity ) || ISBLANK( MailingCountry )</errorConditionFormula>
    <errorMessage>“Contact data is not completed. Please make sure that Street, City and Country fields are not blank.”</errorMessage>
</ValidationRule>
